<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="bk" class="com.book.bean.Book">
<property name="bid" value="1511"/>
<property name="name" value="got"/>
<property name="price" value="520"/>
<property name="auth" ref="auth"/>
</bean>

<bean id="auth" class="com.book.bean.Authour">
<property name="name" value="rrmartin"/>
<property name="mobileno" value="45616"/>
</bean>


</beans>